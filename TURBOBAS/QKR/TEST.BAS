   LET ZAN=1
   'LET CAR$="S4 E5 D10 H5 BR5 BU5 R10 D10 L10 F5 E5 E5 H5 H5 G5 F10 U10 G10"
   'LET PLA$="S4 E5 D10 H5 BR5 BU5 R10 D10 L10 F5 E5 E5 H5 H5 G5 F10 U10 G10"
