'TELEPORT #1
   FOR UJKL=100 TO 115
   FOR UJKQ=120 TO 135
   IF UJKL=P AND UJKQ=L THEN P=620:L=110:COLOR 15,2:COLOR 15,0:GOTO CHK
   IF UJKL=X AND UJKQ=Y THEN X=620:Y=110:COLOR 15,2:COLOR 15,0:GOTO CHK
   NEXT
   NEXT
   FOR STP=1 TO 5
   GHZ%=INT(RND * 15)
   GHJ%=INT(RND * 15)
   FBN%=INT(RND * 2)
   IF FBN%=1 THEN PSET(100+GHZ%+1,120+GHJ%+1),10
