   NEXT
'TELEPORT #2
   FOR UJKL=600 TO 615
   FOR UJKQ=100 TO 115
   IF UJKL=P AND UJKQ=L THEN P=90:L=125:COLOR 15,2:COLOR 15,0:GOTO CHK
   IF UJKL=X AND UJKQ=Y THEN X=90:Y=125:COLOR 15,2:COLOR 15,0:GOTO CHK
   NEXT
   NEXT
   FOR STP=1 TO 5
   GHZ%=INT(RND * 15)
   GHJ%=INT(RND * 15)
   FBN%=INT(RND * 2)
   IF FBN%=1 THEN PSET(600+GHZ%+1,100+GHJ%+1),10
   IF FBN%=0 THEN CIRCLE(600+GHZ%+1,100+GHJ%+1),1,0
