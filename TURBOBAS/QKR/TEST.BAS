   END
CSHOT:
   IF SGS<2 THEN GOTO POO
   SOUND 1000,1
   IF Z=1 THEN DRAW"TA90"
   IF Z=2 THEN DRAW"TA-90"
   IF Z=3 THEN DRAW"TA0"
   IF Z=4 THEN DRAW"TA180"
   PSET(P,L),0
   DRAW"C14 BR13 BD13 R30 H2 R30 H2 R30 H2 R30 H2 R30 H2 R30 H2 R30 H2 R30"
   PSET(P,L),0
   DRAW"C14 BR13 BD12 R30 H2 R30 H2 R30 H2 R30 H2 R30 H2 R30 H2 R30 H2 R30"
   PSET(P,L),0
   DRAW"C14 BR13 BD14 R30 H2 R30 H2 R30 H2 R30 H2 R30 H2 R30 H2 R30 H2 R30"
   FOR N=1 TO 50000
   NEXT N
   SGS=SGS-2
   LET ZA=1
   GOTO POO
   END
AXE:
   PSET(P,L),0
   DRAW"C6"
   IF Z=1 THEN DRAW"TA90"
   IF Z=2 THEN DRAW"TA-90"
   IF Z=3 THEN DRAW"TA0"
   IF Z=4 THEN DRAW"TA180"
   DRAW WEP$
   FOR LKJ=1 TO 10000
   NEXT LKJ
   LET ZA=1
   LET AXE=1
   GOTO POO
   END
BOMB:
   NK=P
   NI=L
   IF BM<1 THEN GOTO POO
   FOR UI=1 TO 100
   PSET(NK,NI),0
   DRAW"C0 BR10 E3 F3 G3 H3"
   FOR N=1 TO 1000
   NEXT N
   IF Z=1 THEN NI=NI-1
   IF Z=2 THEN NI=NI+1
   IF Z=3 THEN NK=NK+1
   IF Z=4 THEN NK=NK-1
   PSET(NK,NI),0
   DRAW"C15 BR10 E3 F3 G3 H3"
   FOR N=1 TO 1000
   NEXT N
   NEXT UI
   FOR F=1 TO 25
   CIRCLE(NK,NI),F,12
   NEXT
   FOR F=1 TO 25
   CIRCLE(NK-10,NI+10),F,12
   NEXT
   FOR F=1 TO 20
   CIRCLE(NK-25,NI-10),F,12
   NEXT
   ZA=1
   BM=BM-1
   GOTO POO
   END
BFG:
   IF SGS<30 THEN GOTO POO
   LET GRP=P
   LET GRZ=L
'START
   DO
   FOR KRT=1 TO 5
   CIRCLE(GRP+2,GRZ+2),KRT,0
   NEXT KRT
   LINE(GRP,GRZ)-(X,Y),0
   LOCATE 1,65
   PRINT"HEALTH:";CINT(HEL);"%  "
   LOCATE 2,65
   PRINT"HEALTH:";CINT(HELQ);"%  "
   IF Z=1 THEN GRZ=GRZ-1
   IF Z=2 THEN GRZ=GRZ+1
   IF Z=3 THEN GRP=GRP+1
   IF Z=4 THEN GRP=GRP-1
   FOR KRT=1 TO 5
   CIRCLE(GRP+2,GRZ+2),KRT,10
   NEXT KRT
   LINE(GRP,GRZ)-(X,Y),10
   IF (GRP-X)<150 AND (GRZ-Y)<150 THEN HELQ=HELQ-0.05
   IF (X-GRP)<150 AND (Y-GRZ)<150 THEN HELQ=HELQ-0.05
   IF (GRP-X)>150 AND (GRZ-Y)>150 THEN HELQ=HELQ-0.05
   IF (X-GRP)>150 AND (Y-GRZ)>150 THEN HELQ=HELQ-0.05
   IF (GRP-X)<150 AND (GRZ-Y)>150 THEN HELQ=HELQ-0.05
   IF (X-GRP)<150 AND (Y-GRZ)>150 THEN HELQ=HELQ-0.05
   IF (GRP-X)>150 AND (GRZ-Y)<150 THEN HELQ=HELQ-0.05
   IF (X-GRP)>150 AND (Y-GRZ)<150 THEN HELQ=HELQ-0.05
   FOR KLL=1 TO 1000
   NEXT KLL
   LOOP UNTIL GRP>640 OR GRP<0 OR GRZ>480 OR GRZ<0
'END
   SGS=SGS-30
   ZA=0.1
   GOTO POO
   END
