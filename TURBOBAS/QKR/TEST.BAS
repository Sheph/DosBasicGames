   LINE(100,400)-(120,420),15,B
   LINE(100,400)-(120,420),15
'BOMBS
   LINE(100,50)-(120,80),7,BF
   LINE(100,50)-(120,80),15
   LINE(120,50)-(100,80),15
   LINE(110,50)-(110,80),15
'LAVA&SLIME
   LINE(380,100)-(500,400),4,BF
   LINE(150,32)-(300,300),2,BF
   LINE(150,400)-(300,450),2,BF
'LAVA
   FOR LI=380 TO 500 STEP+O
   FOR LP=100 TO 400 STEP+M+RND*3
   PSET(LI,LP),12
   O=O-1
   M=M-1
   IF M<2 THEN M=10
   IF O<2 THEN O=10
   NEXT:NEXT
'SLIME
   FOR LIA=150 TO 300 STEP+OA
   FOR LPA=32 TO 300 STEP+MA+RND*3
   PSET(LIA,LPA),10
   OA=OA-1
   MA=MA-1
   IF MA=1 THEN MA=10
   IF OA=1 THEN OA=10
   NEXT
   NEXT
'2
   FOR LIAQ=150 TO 300 STEP+OQ
   FOR LPAQ=400 TO 450 STEP+MQ+RND*3
   PSET(LIAQ,LPAQ),10
   OQ=OQ-1
   MQ=MQ-1
   IF MQ=1 THEN MQ=10
   IF OQ=1 THEN OQ=10
   NEXT
   NEXT
START:
   PUT(P-21,L),BOX%,PSET
   PUT(X-21,Y),BOO%,PSET
   IF HEL < 1 THEN GOTO BOOM
   IF HELQ < 1 THEN GOTO BOOMQ
'CHECK
   FOR MJ=P-10 TO P+10
   FOR LM=L-10 TO L+10
   IF BIO=1 THEN GOTO PLA
   IF POINT(MJ,LM)=4 THEN HEL=HEL-0.05
PLA:
   IF POINT(MJ,LM)=3 THEN HEL=HEL-0.2
   IF POINT(MJ,LM)=12 THEN HEL=HEL-0.08
   IF POINT(MJ,LM)=14 THEN HEL=HEL-0.05
   IF POINT(MJ,LM)=6 THEN HEL=HEL-0.08
   'IF AXEQ=1 AND P=X THEN GOTO ARTH
   'IF AXEQ=1 AND (X-P)<30 AND X>P AND ZAN=4 THEN HEL=HEL-0.03
   'IF AXEQ=1 AND (P-X)<30 AND P>X AND ZAN=3 THEN HEL=HEL-0.03
   'IF AXEQ=1 AND (L-Y)<30 AND L>Y AND ZAN=2 THEN HEL=HEL-0.03
   'IF AXEQ=1 AND (Y-L)<30 AND Y>L AND ZAN=1 THEN HEL=HEL-0.03
   'IF AXEQ=1 AND Y=L AND P=X THEN HEL=HEL-0.03
   NEXT
   NEXT
ARTH:
   FOR MJ=X-10 TO X+10
   FOR LM=Y-10 TO Y+10
   IF BIOQ=1 THEN GOTO PLAQ
   IF POINT(MJ,LM)=4 THEN HELQ=HELQ-0.05
PLAQ:
   IF POINT(MJ,LM)=3 THEN HELQ=HELQ-0.2
   IF POINT(MJ,LM)=12 THEN HELQ=HELQ-0.08
   IF POINT(MJ,LM)=14 THEN HELQ=HELQ-0.05
   IF POINT(MJ,LM)=6 THEN HELQ=HELQ-0.08
   'IF AXE=1 AND P=X THEN GOTO OLKI
   'IF AXE=1 AND (X-P)<30 AND X>P AND Z=3 THEN HELQ=HELQ-0.03
   'IF AXE=1 AND (P-X)<30 AND P>X AND Z=4 THEN HELQ=HELQ-0.03
   'IF AXE=1 AND (L-Y)<30 AND L>Y AND Z=1 THEN HELQ=HELQ-0.03
   'IF AXE=1 AND (Y-L)<30 AND Y>L AND Z=2 THEN HELQ=HELQ-0.03
   'IF AXE=1 AND Y=L AND P=X THEN HELQ=HELQ-0.03
   NEXT
   NEXT
OLKI:
'CHECK
   COLOR 15,0
   IF HEL<30 THEN COLOR 12,0
   LOCATE 1,65
   PRINT"HEALTH:";CINT(HEL);"%  "
   LOCATE 2,65
   COLOR 15
   TIMEPAST = TIMER - StartTime
   COLOR 15
   LOCATE 1,1
   PRINT"SHELLS:";CINT(AM)
   LOCATE 1,19
   PRINT"CELLS:";CINT(SGS)
   LOCATE 1,35
   PRINT"BOMBS:";CINT(BM)
   COLOR 14
   LOCATE 1,51
   PRINT"FRAGS:";FRA
   LOCATE 2,1
   COLOR 15
   PRINT"SHELLS:";CINT(AMQ)
   LOCATE 2,19
   PRINT"CELLS:";CINT(SGSQ)
   LOCATE 2,35
   PRINT"BOMBS:";CINT(BMQ)
   COLOR 14
   LOCATE 2,51
   PRINT"FRAGS:";FRAQ
   COLOR 15
   IF HELQ<30 THEN COLOR 12,0
   LOCATE 2,65
   PRINT"HEALTH:";CINT(HELQ);"%  "
   LET A$=INKEY$
'1P
   IF A$=CHR$(0)+"H" THEN L=L-8:Z=1
   IF A$=CHR$(0)+"P" THEN L=L+8:Z=2
   IF A$=CHR$(0)+"M" THEN P=P+8:Z=3
   IF A$=CHR$(0)+"K" THEN P=P-8:Z=4
   IF A$=CHR$(27) THEN END
   IF A$=CHR$(0)+";" THEN GOTO INFOA
   IF A$="B" OR A$="b" THEN ZA=-1:LOCATE 3,31:COLOR 8:PRINT"QUAD FIRE(1 SHOT)":SOUND 1000,2:DELAY 1:CLS
   IF A$="A" OR A$="a" THEN GOTO INFINIT
   IF A$="`" OR A$="~" THEN GOTO CFG
   IF A$="P" OR A$="p" THEN LOCATE 15,35:COLOR 15:PRINT"PAUSE":DO:LOOP WHILE INKEY$="":LOCATE 15,35:PRINT"     "
   IF A$=CHR$(0)+"R" THEN GOTO SHOT
   IF A$=CHR$(0)+"S" THEN GOTO BSHOT
   IF A$=CHR$(0)+"G" THEN GOTO CSHOT
   IF A$=CHR$(0)+"Q" THEN GOTO AXE
   IF A$=CHR$(0)+"O" THEN GOTO BOMB
   IF A$=CHR$(0)+"I" THEN GOTO BFG
   IF L < 2 THEN L=L+10
   IF L > 460 THEN L=L-10
   IF P > 640 THEN P=P-10
   IF P < 2 THEN P=P+10
   'IF POINT(P+1,L)=4 THEN HEL=HEL-5:LOCATE 1,65:PRINT"                ":LOCATE 1,65:COLOR 5:PRINT"HEALTH:"CINT(HEL);"%":COLOR 15,4:LOCATE 1,75:COLOR 0:PRINT"PP":RANDOMIZE TIMER:CV%=INT(RND * 2):IF CV%=1 THEN SOUND 100,1
   IF BIO=1 THEN GOTO PLK
   IF POINT(P+1,L)=2 THEN HEL=HEL-0.4:LOCATE 1,65:PRINT"                   ":LOCATE 1,65:COLOR 10:PRINT"HEALTH:"CINT(HEL);"%":LOCATE 1,77:COLOR 0:PRINT"PP":RANDOMIZE TIMER:CV%=INT(RND * 2):IF CV%=1 THEN SOUND 50,1
PLK:
   IF POINT(P+1,L+1)=15 OR POINT(P+1,L)=8 AND Z=3 THEN P=P-6
   IF POINT(P+1,L+1)=15 OR POINT(P+1,L)=8 AND Z=1 THEN L=L+6
   IF POINT(P+1,L+1)=15 OR POINT(P+1,L)=8 AND Z=2 THEN L=L-6
   IF POINT(P+1,L+1)=15 OR POINT(P+1,L)=8 AND Z=4 THEN P=P+6
   IF HEL>199 THEN GOTO ZQ
   IF POINT(P+1,L)=9 THEN HEL=HEL+0.4
ZQ:
   IF SGS>49 THEN GOTO ZC
   IF POINT(P+1,L)=13 THEN SGS=SGS+0.4
ZC:
   IF AM>99 THEN GOTO ZT
   IF POINT(P+1,L)=1 THEN AM=AM+0.4
ZT:
   IF BM>24 THEN GOTO ZX
   IF POINT(P+1,L)=7 THEN BM=BM+0.4
ZX:
'2P
   IF A$="S" OR A$="s" THEN Y=Y-8:ZAN=1
   IF A$="X" OR A$="x" THEN Y=Y+8:ZAN=2
   IF A$="C" OR A$="c" THEN X=X+8:ZAN=3
   IF A$="Z" OR A$="z" THEN X=X-8:ZAN=4
   IF A$="R" OR A$="r" THEN GOTO SHOTQ
   IF A$="F" OR A$="f" THEN GOTO BSHOTQ
   IF A$="T" OR A$="t" THEN GOTO CSHOTQ
   IF A$="H" OR A$="h" THEN GOTO AXEQ
   IF A$="G" OR A$="g" THEN GOTO BOMBQ
   IF A$="Y" OR A$="y" THEN GOTO BFGQ
   IF Y < 2 THEN Y=Y+10
   IF Y > 460 THEN Y=Y-10
   IF X > 640 THEN X=X-10
   IF X < 2 THEN X=X+10
   'IF POINT(X+1,Y)=4 THEN HELQ=HELQ-5:LOCATE 2,65:PRINT"                ":LOCATE 2,65:COLOR 5:PRINT"HEALTH:"CINT(HELQ);"%":COLOR 15,4:LOCATE 2,75:COLOR 0:PRINT"PP":RANDOMIZE TIMER:CV%=INT(RND * 2):IF CV%=1 THEN SOUND 100,1
   IF BIOQ=1 THEN GOTO PLKQ
   IF POINT(X+1,Y)=2 THEN HELQ=HELQ-0.4:LOCATE 2,65:PRINT"                   ":LOCATE 2,65:COLOR 10:PRINT"HEALTH:"CINT(HELQ);"%":LOCATE 2,77:COLOR 0:PRINT"PP":RANDOMIZE TIMER:CV%=INT(RND * 2):IF CV%=1 THEN SOUND 50,1
PLKQ:
   IF POINT(X+1,Y+1)=15 OR POINT(X+1,Y)=8 AND ZAN=3 THEN X=X-6
   IF POINT(X+1,Y+1)=15 OR POINT(X+1,Y)=8 AND ZAN=1 THEN Y=Y+6
   IF POINT(X+1,Y+1)=15 OR POINT(X+1,Y)=8 AND ZAN=2 THEN Y=Y-6
   IF POINT(X+1,Y+1)=15 OR POINT(X+1,Y)=8 AND ZAN=4 THEN X=X+6
   IF HELQ>199 THEN GOTO ZI
   IF POINT(X+1,Y)=9 THEN HELQ=HELQ+0.4
ZI:
   IF SGSQ>49 THEN GOTO ZIN
   IF POINT(X+1,Y)=13 THEN SGSQ=SGSQ+0.4
ZIN:
   IF AMQ>99 THEN GOTO ZINA
   IF POINT(X+1,Y)=1 THEN AMQ=AMQ+0.4
ZINA:
   IF BMQ>24 THEN GOTO ARGA
   IF POINT(X+1,Y)=7 THEN BMQ=BMQ+0.4
ARGA:
   IF GHN>0 THEN GOTO CHK
   LET GHN=GHN+ZA
    'LINE(-0.5,-1)-(639,35),8,BF
    'LINE(-0.5,-1)-(639,35),15,B
   GOTO POO
   END
BOOM:
   COLOR 15,0
   FOR F=1 TO 25
   CIRCLE(P,L),F,0
   NEXT
   FOR F=1 TO 25
   CIRCLE(P-10,L+10),F,0
   NEXT
   FOR F=1 TO 20
   CIRCLE(P-25,L-10),F,0
   NEXT
   BEEP
   LET HEL=100
   LET SGS=0
   LET BM=0
   LET AM=20
   LET BIO=0
   DELAY 0.5
   LOCATE 2,35
   COLOR 8,0
   PRINT DX$;" died!"
   LET FRAQ=FRAQ+1
   DELAY 1
   CLS
   ZAR=0
   P=AI
   L=AB
   GOTO SPAWN
   END
BOOMQ:
   COLOR 15,0
   FOR F=1 TO 25
   CIRCLE(X,Y),F,0
   NEXT
   FOR F=1 TO 25
   CIRCLE(X-10,Y+10),F,0
   NEXT
   FOR F=1 TO 20
   CIRCLE(X-25,Y-10),F,0
   NEXT
   BEEP
   LET HELQ=100
   LET SGSQ=0
   LET AMQ=20
   LET BMQ=0
   LET BIOQ=0
   DELAY 0.5
   LOCATE 2,35
   COLOR 8,0
   PRINT DN$;" died!"
   LET FRA=FRA+1
   DELAY 1
   CLS
   ZAR=0
   X=BI
   Y=BB
   GOTO SPAWN
   END
SHOT:
   IF AM=0 THEN GOTO POO
   SOUND 120,1
   IF Z=1 THEN DRAW"TA90":DRAW"C4 BR13 BD13 R80"
   IF Z=2 THEN DRAW"TA-90":DRAW"C4 BR13 BD13 R80"
   IF Z=3 THEN DRAW"TA0":DRAW"C4 BR13 BD13 R80"
   IF Z=4 THEN DRAW"TA180":DRAW"C4 BR13 BD13 R80"
   FOR N=1 TO 10000
   NEXT N
   AM=AM-1
   LET ZA=1
   GOTO POO
   END
BSHOT:
   IF SGS<5 THEN GOTO POO
   SOUND 200,2
   IF Z=1 THEN GOTO ARG
   IF Z=2 THEN GOTO ARH
   IF Z=3 THEN GOTO ARI
   IF Z=4 THEN GOTO ARM
   END
ARI:
   LET I=P
   LET LO=2
   FOR I=P+30 TO P+180 STEP+10
   CIRCLE(I,L),LO,3
   LET LO=LO+1
   NEXT I
   DELAY 0.3
   SGS=SGS-5
   LET ZA=1
   GOTO POO
   END
ARM:
   LET I=P
   LET LO=2
   FOR I=P-30 TO P-180 STEP-10
   CIRCLE(I,L),LO,3
   LET LO=LO+1
   NEXT I
   DELAY 0.3
   SGS=SGS-5
   LET ZA=1
   GOTO POO
   END
