   CLOSE #1
   OPEN"MODEL1.MDL" FOR INPUT AS #2
   INPUT #2,CAR$
   CLOSE #2
   OPEN"MODEL2.MDL" FOR INPUT AS #2
   INPUT #2,PLA$
