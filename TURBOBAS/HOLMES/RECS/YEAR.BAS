   CLS
   SCREEN 12
   DIM YEAR(5000)
   PAINT(1,1),4,15
   PRINT"в 2000 году"
   FOR A=0 TO 90
   FOR B=0 TO 20
   IF POINT(A,B)=15 THEN CIRCLE(A*2+100,B*2+100),0.5,7:PSET(A*2+100,B*2+101),8
   NEXT
   NEXT
   GET(95,95)-(278,135),YEAR
   DEF SEG=VARSEG(YEAR(0))
   BSAVE"YEAR.HMS",VARPTR(YEAR(0)),10000